  "version": "0.0.1",
  "description": "Test library for Fluxlet",
  "main": "lib/testlet",
  "dependencies": {},
  "devDependencies": {
    "babel": "^5.8.23",
    "glob": "^5.0.14",
    "groc": "^0.8.0",
    "jspm": "^0.16.8",
    "karma": "^0.13.10",
    "karma-chrome-launcher": "^0.2.0",
    "karma-cli": "^0.1.0",
    "karma-firefox-launcher": "^0.1.6",
    "karma-jspm": "^2.0.1",
    "karma-mocha": "^0.2.0",
